    "cross-env": "^10.1.0",
    "cross-fetch": "^4.1.0",
    "graphql": "^16.12.0",
    "next": "^15.4.10",
    "payload": "^3.68.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "22.5.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint-config-next": "16.0.9",
