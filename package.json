{
  "name": "venue-iq",
  "packageManager": "npm@11.6.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation turbo dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation turbo build",
    "build:web": "cross-env NODE_OPTIONS=--no-deprecation turbo build --filter=venue-iq-web",
    "build:cms": "cross-env NODE_OPTIONS=--no-deprecation turbo build --filter=venue-iq-cms",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation turbo lint",
    "typecheck": "cross-env NODE_OPTIONS=--no-deprecation turbo typecheck",
    "clean": "cross-env NODE_OPTIONS=--no-deprecation turbo clean && rimraf node_modules .turbo",
    "full-check": "cross-env NODE_OPTIONS=--no-deprecation npm run lint && npm run typecheck && npm run build"
  },
  "dependencies": {
    "cross-env": "^10.1.0",
    "@venue-iq/shared": "0.0.0",
    "@venue-iq/ui": "0.0.0",
    "@venue-iq/cms-types": "0.0.0"
  },
  "devDependencies": {
    "turbo": "^2.6.3",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "eslint": "^9.39.2",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0"
  }
}
